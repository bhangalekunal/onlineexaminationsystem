import { EventEmitter, OnDestroy, OnInit } from '@angular/core';
import { SidebarAccordionComponent } from './sidebar-accordion/sidebar-accordion.component';
import { SidebarHeaderComponent } from "./sidebar-header.component";
import { SidebarContentComponent } from "./sidebar-content.component";
import { SidebarOpenedEventArgs } from "./sidebar-opened.event-args";
import { SidebarMouseTouchEventArgs } from "./sidebar-mouse-touch.event-args";
export declare class SidebarComponent implements OnInit, OnDestroy {
    private _container;
    position: 'left' | 'right' | 'top' | 'bottom';
    classNameSidebar: boolean;
    className: string;
    style: CSSStyleDeclaration;
    headerClicked: EventEmitter<SidebarComponent>;
    headerTouchMoved: EventEmitter<SidebarMouseTouchEventArgs>;
    headerTouchEnded: EventEmitter<SidebarMouseTouchEventArgs>;
    openedChange: EventEmitter<SidebarOpenedEventArgs>;
    classNameSidebarOpened: boolean;
    private _headers;
    private _contents;
    constructor(_container: SidebarAccordionComponent);
    private _opened;
    get opened(): boolean;
    set opened(value: boolean);
    get _headersLength(): number;
    get _contentsLength(): number;
    ngOnInit(): void;
    ngOnDestroy(): void;
    _addHeader(header: SidebarHeaderComponent): void;
    _removeHeader(header: SidebarHeaderComponent): void;
    _addContent(content: SidebarContentComponent): void;
    _removeContent(content: SidebarContentComponent): void;
    open(): void;
    close(): void;
    private subscribe;
    private unsubscribe;
}
